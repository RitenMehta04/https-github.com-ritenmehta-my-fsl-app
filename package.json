{
  "name": "my-fsl-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "lint": "npx eslint . --ext .js --max-warnings=0",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "echo 'Building...' && exit 0",
    "prepare": "husky",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint --fix",
    "__tests__/**/*.{js,jsx}": "eslint --fix",
    "*.{js,jsx}": "eslint --fix",
    "*.{json,md,yml,yaml,css,scss}": "prettier --write"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "description": "CI pipeline with ESLint, Jest, and build step.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RitenMehta04/my-fsl-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RitenMehta04/my-fsl-app/issues"
  },
  "homepage": "https://github.com/RitenMehta04/my-fsl-app#readme"
}
